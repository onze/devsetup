alias ga='git add'
alias gap='git add -p'
alias gc='git commit'
alias gl='git log --graph --decorate=auto --branches'
alias gp='git push'
alias gco='git checkout'
alias gb='git branch'
alias gd='git diff'
alias gdc='git diff --cached'
alias gds='git diff --staged'
alias grh='git reset head'
alias gsec='git secret'
